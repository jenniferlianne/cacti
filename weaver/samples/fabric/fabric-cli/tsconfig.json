{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "scripthost",
      "es2015.promise",
      "es2015.generator",
      "es2015.iterable",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "outDir": "dist",
    "strict": false,
    "target": "es5",
    "declaration": true,
    "declarationDir": "build/types"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "references": [
    {
      "path": "../../../common/protos-js/tsconfig.json"
    },
    {
      "path": "../../../sdks/fabric/interoperation-node-sdk/tsconfig.json"
    }
  ]

}
