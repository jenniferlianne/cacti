{
  "name": "@hyperledger/cacti-plugin-copm-fabric",
  "version": "0.0.1",
  "description": "Cacti Common Operators - Fabric",
  "scripts": {
    "test": "yarn run --top-level test:jest:all ./src/test/typescript/*/*.test.ts",
    "test:lock": "yarn run --top-level test:jest:all ./src/test/typescript/integration/test-copm-lock-claim.test.ts",
    "test:provestate": "yarn run --top-level test:jest:all ./src/test/typescript/integration/test-copm-provestate.test.ts",
    "test:pledge": "yarn run --top-level test:jest:all ./src/test/typescript/integration/test-copm-pledge-claim.test.ts",
    "codegen:proto": "yarn run proto:copy; yarn run proto:connectrpc",
    "proto:copy": "/bin/bash ../cacti-copm-core/export-protos.sh . fabric",
    "proto:connectrpc": "yarn run buf generate --template=./buf.gen.yaml ./src/main/proto/generated/openapi",
    "dev:redocopm": "cd ../cacti-copm-core && yarn run bundleopenapi && yarn run codegen:proto",
    "dev:getweaverdeps": "cp ../../weaver/common/protos/common/asset_locks.proto ./src/main/proto/weaver-protos",
    "proto:weaver": "yarn run buf generate --template=./weaver-buf.gen.yaml ./src/main/proto/weaver-protos"
  },
  "dependencies": {
    "@bufbuild/buf": "1.30.0",
    "@bufbuild/protobuf": "1.10.0",
    "@connectrpc/connect": "^1.4.0",
    "@hyperledger/cacti-copm-core": "workspace:^",
    "@hyperledger/cacti-weaver-protos-js": "2.0.0-rc.4",
    "@hyperledger/cacti-weaver-sdk-fabric": "workspace:^",
    "@hyperledger/cactus-common": "2.0.0-rc.4",
    "@hyperledger/cactus-core": "2.0.0-rc.4",
    "@hyperledger/cactus-core-api": "2.0.0-rc.4",
    "fabric-ca-client": "^2.2.20",
    "fabric-network": "^2.2.20"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "1.8.0",
    "@connectrpc/connect-node": "^1.4.0",
    "@connectrpc/protoc-gen-connect-es": "^1.4.0",
    "@hyperledger/cactus-cmd-api-server": "2.0.0-rc.4",
    "@hyperledger/cactus-plugin-keychain-memory": "2.0.0-rc.4",
    "@hyperledger/cactus-plugin-ledger-connector-fabric": "workspace:^",
    "@hyperledger/cactus-test-tooling": "2.0.0-rc.4",
    "@openapitools/openapi-generator-cli": "2.13.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger/cacti.git"
  },
  "author": "Jennifer Bell",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hyperledger/cacti/issues"
  },
  "homepage": "https://github.com/hyperledger/cacti#readme"
}
